{
  "name": "@peaky/bundler",
  "version": "0.0.1",
  "description": "Peaky bundler - minimalistic bundler written in ReasonML",
  "license": "MIT",
  "esy": {
    "build": "dune build -p bundler",
    "buildDev": "refmterr dune build --root . --only-package bundler",
    "install":
      "esy-installer #{self.target_dir / 'default' /  'bundler.install'}",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": { "bin": { "peaky": "Hello" } },
    "buildEnv": { "ODOC_SYNTAX": "re" }
  },
  "scripts": { "test": "esy x Hello", "doc": "esy dune build @doc" },
  "dependencies": {
    "@esy-ocaml/reason": ">= 3.4.0 < 3.6.0",
    "@opam/dune": "*",
    "@opam/lwt": "5.1.1",
    "@opam/menhir": "20200211",
    "@reason-native/console": "*",
    "@reason-native/file-context-printer": "^0.0.3",
    "@reason-native/pastel": "*",
    "ocaml": "~4.8.1000"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "@reason-native/rely": "*",
    "refmterr": "*",
    "ocaml": "~4.8.1000",
    "@opam/odoc": "*"
  }
}